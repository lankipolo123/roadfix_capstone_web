{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "main.js", "use": "@vercel/static" },
    { "src": "global.css", "use": "@vercel/static" },
    { "src": "src/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*\\.js|.*\\.css|.*\\.ico|.*\\.png)", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
